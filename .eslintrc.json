{
  "env": {
    "commonjs": true,
    "es2021": true,
    "node": true
  },
  "extends": "airbnb-base",
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest"
  },
  "rules": {
    "settings": {
      "import/resolver": {
        "node": {
          "extensions": [".ts", ".tsx"],
          "moduleDirectory": ["src", "node_modules"]
        }
      }
    }
  }
}
